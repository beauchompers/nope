# .env.example - Required environment variables for NOPE

# Database
DB_USER=nope
DB_PASSWORD=<auto-generated>

# Security (REQUIRED - app will not start without these)
SECRET_KEY=<auto-generated>

# Default admin account (REQUIRED - minimum 6 characters)
DEFAULT_ADMIN_USER=admin
DEFAULT_ADMIN_PASSWORD=<auto-generated>

# EDL basic auth (REQUIRED - minimum 6 characters)
DEFAULT_EDL_USER=edl
DEFAULT_EDL_PASSWORD=<auto-generated>
